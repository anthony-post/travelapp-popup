elect2\n        },\n        props: {},\n        data() {\n            return {\n                title: 'Giorni, luoghi e orari',\n                description: 'Quando si svolge il servizio',\n                departure: 'Orario di partenza da Cervinia o incontro',\n                place: 'Luogo di partenza o incontro con i clienti',\n                options: [\n                    {name: '15:00', value: 1},\n                    {name: '16:00', value: 2},\n                    {name: '17:00', value: 3}\n                ],\n                options2: [\n                    {name: '18:00', value: 1},\n                    {name: '19:00', value: 2},\n                    {name: '20:00', value: 3}\n                ],\n                selected: '15:00',\n                selected2: '18:00'\n            }\n        },\n        methods: {\n            optionSelect(option) {\n                this.selected = option.name;\n            },\n            optionSelect2(option2) {\n                this.selected2 = option2.name;\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/components"}]}