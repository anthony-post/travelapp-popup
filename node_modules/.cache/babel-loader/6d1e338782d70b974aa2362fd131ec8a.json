ectedTab = tab\" \n                :class=\"{ activeTab: selectedTab === tab }\">\n                {{ tab }}\n            </li>\n        </ul>\n\n        <div \n            class=\"v-block-giorni\"\n            v-show=\"selectedTab === 'Giorni'\"> <!--при выполнении условия v-show этот блок показывается-->\n\n            <input type=\"checkbox\" class=\"custom-checkbox Lu\" id=\"Lu\" name=\"day\" value=\"yes\">\n            <label for=\"Lu\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox Ma\" id=\"Ma\" name=\"day\" value=\"yes\">\n            <label for=\"Ma\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox\" id=\"Me\" name=\"day\" value=\"yes\">\n            <label for=\"Me\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox\" id=\"Gi\" name=\"day\" value=\"yes\">\n            <label for=\"Gi\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox\" id=\"Ve\" name=\"day\" value=\"yes\">\n            <label for=\"Ve\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox\" id=\"Sa\" name=\"day\" value=\"yes\">\n            <label for=\"Sa\"></label>\n            <input type=\"checkbox\" class=\"custom-checkbox\" id=\"Do\" name=\"day\" value=\"yes\">\n            <label for=\"Do\"></label>\n\n            <!-- <input type=\"checkbox\">\n            <input type=\"checkbox\">\n            <input type=\"checkbox\">\n            <input type=\"checkbox\">\n            <input type=\"checkbox\">\n            <input type=\"checkbox\">\n            <input type=\"checkbox\"> -->\n        </div>\n\n        <div \n            class=\"v-block-indata\"\n            v-show=\"selectedTab === 'In data'\"> <!--при выполнении условия v-show блок показывается-->\n\n            <p class=\"v-text v-text-position-indata\">Seleziona la data dell'evento</p>\n\n            <v-select-date\n                :optionsDate='optionsDate'\n                @selectDate=\"optionSelectDate\"\n                :selectedDate=\"selectedDate\">\n            </v-select-date>\n        </div>\n\n    </div>\n</template>\n\n<script>\n    import VSelectDate from './v-select-date.vue'\n\n    export default {\n        name: 'v-tabs',\n        components: {\n            VSelectDate\n            },\n        props: {},\n        data() {\n            return {\n                tabs: ['Giorni', 'In data'], //загаловки вкладок\n                selectedTab: 'Giorni', //см. выше - по клику @click на вкладку в это свойство будет записываться значение tab с названием активной вкладки\n                optionsDate: [\n                    {name: '20.12.2020', value: 1},\n                    {name: '21.12.2020', value: 2},\n                    {name: '22.12.2020', value: 3}\n                ],\n                selectedDate: '22.12.2020'\n            }\n        },\n        methods: {\n            optionSelectDate(optionDate) {\n                this.selectedDate = optionDate.name;\n            }\n        }\n    }\n</script>\n"],"sourceRoot":"src/components"}]}