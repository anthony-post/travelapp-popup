type: Array,\n                default() {\n                    return []\n                }\n            },\n            selected: {\n                type: String,\n                default: ''\n            }\n        },\n        data() {\n            return {\n                areOptionsVisible: false\n            }\n        },\n        methods: {\n            selectOption(option) {\n                // console.log(option);\n                this.$emit('select', option);\n                this.areOptionsVisible = false;\n            },\n            hideSelect() {\n                this.areOptionsVisible = false;\n            }\n        },\n        mounted() {\n            document.addEventListener('click', this.hideSelect.bind(this), true);\n        },\n        beforeDestroy() {\n            document.removeEventListener('click', this.hideSelect);\n        }\n    }\n</script>\n\n<style>\n\n    /* .v-select {\n        position: relative;\n        width: 168px;\n        cursor: pointer;\n    } */\n\n    /* .v-select-title {\n        margin: 0;\n        background: #FFFFFF;\n        border: 1px solid #F5F5F5;\n        box-sizing: border-box;\n        box-shadow: 0px 4px 4px rgba(218, 218, 218, 0.1);\n        border-radius: 4px;\n        padding: 15px 0 15px 0;\n        text-align: center;\n    } */\n\n    /* .v-select-options {\n        border: 0.5px solid #EFEEEC;\n        position: absolute;\n        top: 20px;\n        right: 0;\n        width: 100%;\n    } */\n\n    /* .v-select-options p:hover {\n        background: #e7e7e7;\n    } */\n    \n</style>"]}]}